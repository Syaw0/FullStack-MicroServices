{
  "name": "fullstack-microservices-web",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": " vite",
    "build": " tsc && vite build",
    "preview": "vite preview",
    "server": "ts-node -P ./tsconfig2.json ./server/index.ts",
    "devServer": "nodemon",
    "test": "jest --watchAll",
    "buildServer": "tsc -p ./tsconfig2.json"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  },
  "nodemonConfig": {
    "watch": [
      "./server/*"
    ],
    "exec": "npm run server",
    "ext": "ts"
  }
}
